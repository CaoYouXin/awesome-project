{
  "easycom": {
    "autoscan": true,
    "custom": {
      "^uni-(.*)": "@/uni-modules/uni-$1/uni-$1.vue",
      "^u-(.*)": "uview-plus/components/u-$1/u-$1.vue"
    }
  },
  "pages": [
    {
      "path": "pages/index/index",
      "style": {
        "navigationBarTitleText": "道医九宫格"
      }
    },
    {
      "path": "pages/awesome/time/index",
      "style": {
        "navigationBarTitleText": "时间密码"
      }
    },
    {
      "path": "pages/awesome/phone/index",
      "style": {
        "navigationBarTitleText": "手机号密码"
      }
    }
  ],
  "globalStyle": {
    "navigationBarTextStyle": "white",
    "navigationBarTitleText": "道医九宫格",
    "navigationBarBackgroundColor": "#F55F5F",
    "backgroundColor": "#F8F8F8",
    "app-plus": {
      "background": "#efeff4"
    }
  }
}
